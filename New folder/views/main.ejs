<%-include('./partials/header') %>
  <section class="video">
    <% for(var i=0;i<videos.length;i++){ %>
    <video autoplay loop muted style="width: 100%; height: 600px;">
      <source src="<%=videos[i].video%>" type="video/mp4">
    </video>
    <% } %>
    <!-- <iframe width="1520" height="627" src="https://www.youtube.com/embed/qZVTkn2NjS0" title="Marvel Studiosâ€™ Secret Invasion | Official Trailer | Disney+" frameborder="0" allow=" autoplay; "></iframe> -->
  </section>
    <section class="main">
      <div class="container">
        <div class="headingArrow">
          <p>Updated Movies</p>
          <div id="container">
            <a class="button learn-more" href="/browse/newRelease">
              <span class="circle" aria-hidden="true">
                <span class="icon arrow"></span>
              </span>
              <span class="button-text">Explore All</span>
            </a>
          </div>
        </div>
        <div class="swiper-container swap">
          <div class="swiper-wrapper">
            <% for(var i=0;i<updatedMovies.length;i++){ %>
            <div class="swiper-slide imgMovie">
              <a href="/movie/<%=updatedMovies[i].slug%>"><img src="<%= updatedMovies[i].moviePoster %>" alt=""></a>
              <div class="movieSomeDetail">
                <h3><%=updatedMovies[i].title%></h3>
                <h5>Duration : <%=updatedMovies[i].movieDuration%></h5>
              </div>
            </div>
            <% } %>
          </div>
          <div class="arrow">
              <div class="swiper-button-next" style="left: 92rem;"></div>
              <div class="swiper-button-prev"></div>
          </div>
        </div>
      </div>
